{
  "rewrites": [
    { "source": "/:match/:url*", "destination": "https://:match/:url*" },
    { "source": "/:match/:url*/", "destination": "https://:match/:url*/" },
    { "source": "/h/:match/:url*", "destination": "http://:match/:url*" },
    { "source": "/h/:match/:url*/", "destination": "http://:match/:url*/" },
    { "source": "/wss/:match/:url*", "destination": "wss://:match/:url*" }
  ]
}
